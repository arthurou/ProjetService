<!DOCTYPE html>
<html>

<head>

    <meta charset="UTF-8">

    <title>Projet Service</title>

    {% block stylesheets %}
        <link rel="stylesheet" href="{{ asset('bundles/User/css/style.css') }}" type="text/css" media="all" />
    {% endblock %}

    <script src="bundles/User/js/prefixfree.min.js"></script>

</head>

<body>

<div class="body"></div>
<div class="grad"></div>
<div class="header">
    <div>Projet<span>Service</span></div>
</div>

<br>

<div class="login">
    {% trans_default_domain 'FOSUserBundle' %}

    {% block fos_user_content %}
        <form action="{{ path('fos_user_registration_register') }}" {{ form_enctype(form) }} method="POST" class="fos_user_registration_register">
            {{ form_widget(form) }}
            <div>
                <input type="submit" value="{{ 'registration.submit'|trans }}" />
            </div>
        </form>
    {% endblock fos_user_content %}

</div>

<script src='http://codepen.io/assets/libs/fullpage/jquery.js'></script>

</body>

</html>